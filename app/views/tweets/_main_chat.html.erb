<div class="contents row">
  <% @tweets.each do |tweet| %>
    <div class="content_post">
      <span class="name">
        <%= tweet.title %>
      </span>
      <p><%= tweet.text %></p>
      <div class="more">
        <span>
          <%= image_tag 'arrow_top.png' %>
        </span>
        <ul class="more_list">
          <li>
            <%= link_to '削除', tweet_path(tweet.id), method: :delete %>
          </li>
        </ul>
      </div>
    </div>
  <% end %>
</div>