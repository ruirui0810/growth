
<div class="contents row">
  <% @tweets.each do |tweet| %>

    <div class="content_post">
      <span class="name">
        <%= tweet.title %>
      </span>
      <p><%= tweet.text %></p>
    </div>

    <div class="more">
      <ul class="more_list">
        <li>
          <%= link_to '削除', tweet_path(tweet.id), method: :delete %>
        </li>
        <li>
          <%= link_to '編集', edit_tweet_path(tweet.id), method: :get %>
        </li>
      </ul>
    </div>

  <% end %>
</div>